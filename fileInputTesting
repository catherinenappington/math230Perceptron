#include <stdio.h>
#include <stdlib.h>

int main(int argc, char* argv[])
{
    FILE * perceptronInput;

    perceptronInput = fopen("in.csv", "r");

    if(perceptronInput == NULL)
    {
        printf("An error has occurred. Unable to accept the input from the file.");
        return 17;
    }
    int i;
    for(i = 0; i < argc; i++)
        printf("%s\n", *argv[i]);
    return 0;
}
